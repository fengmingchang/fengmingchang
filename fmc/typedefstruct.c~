# include <stdio.h>
 int main ( int  argy , const char *argv[])
{
    typedef struct 
    {
        int age ;
        char name ;
        int  gongling;
        double  salary;
    }empoy;
   empoy jy ={20,"æ˜Œ",3,1020};
   empoy man;
   man.age = 10;
   man.name = 'a';
   man.gongling = 1;
   man.salary = 343;
   
    printf ("%d,%c,%d,%f",man.age,man.name,man.gongling,man.salary);
    printf ("%d,%c,%d,%f",jy.age,jy.name,jy.gongling,jy.salary);

    return 0;
}
